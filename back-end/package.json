{
  "name": "back-end",
  "version": "1.0.0",
  "description": "",
  "main": "./build/server",
  "scripts": {
    "start": "node build/server.js",
    "dev": "tsx watch src/server.ts",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "tsc --build",
    "test": "mocha -r ts-node/register ./src/tests/*.test.ts -t 10000 --exit",
    "docker": "docker run -d --name Postgres-NG -p 5432:5432 -v /tmp/database:/var/lib/postgresql/data -e POSTGRES_PASSWORD=1234 postgres",
    "db:drop": "cd src/database && npx sequelize-cli db:drop",
    "predev": "npm run build && npm run db:reset",
    "prestart": "npm run build && npm run db:reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.3.4",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.0",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "eslint": "^8.27.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-config-xo-typescript": "^0.54.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.1.0",
    "sequelize-cli": "^6.5.2",
    "sinon": "^14.0.2",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
